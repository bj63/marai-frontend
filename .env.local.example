# Copy to .env.local and provide real values. Do not commit your secrets.

# Supabase - Use the anon key only. RLS must be enabled on every table.
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="eyJhbGc..."

# Backend services
NEXT_PUBLIC_API_BASE=http://localhost:5000
NEXT_PUBLIC_API_URL="http://localhost:8000"
NEXT_PUBLIC_FACTIME_WS_URL="ws://localhost:8001"

# Optional WebRTC ICE servers
NEXT_PUBLIC_FACTIME_ICE_SERVERS="[]"

# Blockchain configuration
NEXT_PUBLIC_CHAIN_ID=137
NEXT_PUBLIC_MIRAI_CARD=0xYourCardAddress
NEXT_PUBLIC_MIRAI_COIN=0xYourCoinAddress
NEXT_PUBLIC_MIRAI_MARKETPLACE=0xYourMarketplaceAddress
